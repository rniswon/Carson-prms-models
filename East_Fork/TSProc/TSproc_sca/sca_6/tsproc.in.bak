START SETTINGS
#CONTEXT model_run
CONTEXT pest_prep
DATE_FORMAT mm/dd/yyyy
END SETTINGS

####################################################################
# Observed streamflows are read from an SSF file.
####################################################################
START GET_MUL_SERIES_SSF
 CONTEXT all
 FILE .\EastFk_obs_sca2.txt
 DATE_1 10/01/2000
 TIME_1 00:00:00
 DATE_2 09/30/2015
 TIME_2 00:00:00
 SITE EF2
 NEW_SERIES_NAME EastFork_sca2_obs
END GET_MUL_SERIES_SSF

START GET_MUL_SERIES_STATVAR
 CONTEXT all
 FILE .\statvar_sca.dat
 VARIABLE_NAME subinc_snowcov
 LOCATION_ID 1
 NEW_SERIES_NAME EastFork_sca2_sim
 DATE_1 10/01/2000
 TIME_1 00:00:00
 DATE_2 09/30/2015
 TIME_2 00:00:00
END GET_MUL_SERIES_STATVAR

START SERIES_COMPARE
 CONTEXT all
 SERIES_NAME_SIM EastFork_sca2_sim
 SERIES_NAME_OBS EastFork_sca2_obs
 NEW_C_TABLE_NAME compare_series
 BIAS                        yes
 STANDARD_ERROR              yes
 RELATIVE_BIAS               yes
 RELATIVE_STANDARD_ERROR     yes
 NASH_SUTCLIFFE              yes
 COEFFICIENT_OF_EFFICIENCY   yes
 INDEX_OF_AGREEMENT          yes
 VOLUMETRIC_EFFICIENCY       yes
 EXPONENT 1
END SERIES_COMPARE
 
START LIST_OUTPUT
 CONTEXT all
 FILE EastFork_sca2_Processed_Output.txt
 C_TABLE_NAME compare_series
END LIST_OUTPUT

